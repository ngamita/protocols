  <h:body>
    <group ref="/VS_Soil_Sample_15.09.2014_v1/metadata">
      <group appearance="field-list" ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display">
        <select1 ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/country">
          <label>Country</label>
          <item>
            <label>Ghana</label>
            <value>GHA</value>
          </item>
          <item>
            <label>Tanzania</label>
            <value>TZA</value>
          </item>
        </select1>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/plot_id">
          <label>Plot ID #</label>
          <hint>Optional</hint>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/analysts_fname">
          <label>Analysts First Name</label>
          <hint>First Name of person Weighing Samples</hint>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/analysts_lname">
          <label>Analysts Last Name (Others)</label>
          <hint>Last Name of person Weighing Samples</hint>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/scribe_fname">
          <label>Scribe First Name</label>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/scribe_lname">
          <label>Scibe Last Name (Others)</label>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/metadata/meta_display/data_entry_date">
          <label>Date of Weighing dried samples</label>
        </input>
      </group>
    </group>
    <input ref="/VS_Soil_Sample_15.09.2014_v1/corner_1se">
      <label>CORNER 1 (SE)</label>
    </input>
    <group ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp">
      <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/top_depth">
        <label>TOP SOIL ( 0 â€“ 20 ) Depth (cm)</label>
      </input>
      <group appearance="field-list" ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp">
        <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp/top_depth_sample">
          <label>Depth to Sampling Restriction if any (cm)</label>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp/top_tin_dry_weight">
          <label>Total Weight of Cumulative Mass Sample (g)</label>
          <hint>( to nearest 1.0 g )</hint>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp/top_sample_bag_label">
          <label>Soil Moisture Tin ID #</label>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp/top_total_dry_weight">
          <label>Weight of Soil Moisture Tin</label>
          <hint>( to nearest 0.1 g )</hint>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp/top_fine_dry_weight">
          <label>Weight of Soil Moisture Tin + Fresh Soil sub Sample</label>
          <hint>( to nearest 0.1 g )</hint>
        </input>
        <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/dp1_grp/top_coarse_dry_weight">
          <label>Hand Texture</label>
          <hint>to nearest g</hint>
        </input>
      </group>
      <input ref="/VS_Soil_Sample_15.09.2014_v1/cr1_grp/subsoil_depth">
